public class JavaBasics{
  public static int factorial(int n){
      int fact= 1;
      for(int i=1; i<=n; i++){
          fact= fact*i;
      }
      return fact;
  }
  public static void main (String [] args){
      Scanner sc = new Scanner(System.in);
      int s = sc.nextInt();
      System.out.println(factorial(s));
  }
}
